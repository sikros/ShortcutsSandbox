<view class="UCenter-bg text-shadow">
  <image src="/images/logo.png" class="png" mode="widthFix"></image>
  <view class="text-xl text-bold">捷径沙盒
    <text class="text-df">for iOS</text>
  </view>
  <view class="margin-top-sm">
    <text>@星晨网络</text>
  </view>
  <image src="/images/wave.gif" mode="scaleToFill" class="gif-wave"></image>
</view>
<view class="padding flex text-center text-cut text-grey bg-white shadow-warp" bindtap="CopyLink"
  data-link="{{hitokoto}}">
  <text>{{hitokoto}}</text>
</view>
<view class="cu-list menu card-menu margin-top-xl margin-bottom-xl shadow-lg radius">
  <view class="cu-item arrow">
    <navigator class="content" url="/pages/form/form" hover-class="none">
      
      <text class="cu-avatar sm radius cuIcon-formfill bg-cyan text-white"></text>
      <text class="text-black text-df text-bold margin-left-xs">提交作品</text>
    </navigator>
  </view>
  <view class="cu-item arrow">
    <view class="content" bindtap="showQrcode" data-url="https://work.weixin.qq.com/wework_admin/common_genqrcode?qr_margin=30&use_new_style=true&url=http://weixin.qq.com/r/kR2agiPENX5KrWfh90iD">
      <text class="cu-avatar sm radius cuIcon-weixin  bg-olive text-white"></text>
      <text class="text-black text-df text-bold margin-left-xs" bindtap="CopyLink" data-link="starnettech">微信公众号</text>
    </view>
  </view>
  <view class="cu-item arrow">
    <mcell  style='zoom:0.7' bind:startmessage='startmessage' bind:completemessage="completemessage" url='https://work.weixin.qq.com/wework_admin/common_genqrcode?qr_margin=30&use_new_style=true&url=https://work.weixin.qq.com/ct/wcde13e7fa915d994da7a92dd2da6edff6b1' contactText='业务咨询' iconBorderRadius='10' iconUrl='https://wwcdn.weixin.qq.com/node/wwmng/wwmng/style/images/icons/AppKfApp$bf7e3ee6.svg' />    
    <view class="radius fr">
        <text class="text-right text-xs text-grey">捷径或小程序等业务合作及定制</text>
      </view>
  </view>
  <view class="cu-item arrow">
    <mcell  style='zoom:0.7' bind:startmessage='startmessage' bind:completemessage="completemessage" url='https://work.weixin.qq.com/gm/780f2907377f8cc5bdeb7b70a346fb8b' contactText='加入群聊' iconBorderRadius='10'  />    

    <view class="radius fr">
        <text class="text-right text-xs text-grey">iOS与Mac系统相关技术交流</text>
      </view>
  </view>  
</view>
<view class="padding-xs flex align-center">
  <view class="flex-sub text-center">
    <view class="text-xs padding">      
      <text class="text-gray">当前收录快捷指令<text bindlongpress="login" class="text-blue">{{num}}</text>个</text>     
    </view>
    <view class="text-xs">      
      <text class="text-gray" bindlongpress="admin">本站展示的全部作品均为网友创作并发布,使用前请自行鉴别其安全性,本站不对作品内容及相关风险做出任何承诺。</text>            
    </view>
    
  </view>
</view>

<official-account></official-account>
<view class="cu-tabbar-height"></view>